<?xml version="1.0" encoding="utf-8" ?>
<openerp>
  <data noupdate="1">

  <!-- Main style -->
  <template id="sme_main_less" inherit_id="website.assets_frontend">
    <xpath expr="." position="inside">
      <link href='https://fonts.googleapis.com/css?family=Quicksand:400,300,700' rel='stylesheet' type='text/css' />
      <link href="/theme_sme/static/less/layout.less" rel="stylesheet" type="text/less"/>
      <link href="/theme_sme/static/less/base.less" rel="stylesheet" type="text/less"/>
      <link href="/theme_sme/static/less/home.less" rel="stylesheet" type="text/less"/>
      <link href="/theme_sme/static/less/snippets.less" rel="stylesheet" type="text/less"/>
      <link href="/theme_sme/static/less/contact.less" rel="stylesheet" type="text/less"/>
    </xpath>
  </template>

  <template id="sme_head" inherit_id="website.layout">
    <xpath expr="//head" position="inside">
      <link rel="shortcut icon" type="image/x-icon" href="/theme_sme/static/img/logo_sme.ico" />
    </xpath>
  </template>

  <template id="sme_frontend_js" inherit_id="website.assets_frontend" priority="900">
    <xpath expr="//link[last()]" position="after">
      <script type="text/javascript" src="/theme_sme/static/js/main.js"/>
      <script type="text/javascript" src="/theme_sme/static/js/masonry.js"/>
    </xpath>
  </template>

</data>
</openerp>
